#!/usr/bin/env node
const {
  config: { hide, live, watched },
  initParent,
  initChild,
  initStandalone
} = require('.')
const [node, ganesha, ...argv] = process.argv
if (hide) (
  process.argv = [node, ...argv] // remove Ganesha script
)
if (live) {
  initParent(argv)
} else if (watched) {
  initChild(argv)
} else {
  initStandalone(argv)
}
